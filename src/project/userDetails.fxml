<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="container" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.userDetailsController">
   <children>
      <AnchorPane id="AnchorPane" fx:id="anchorRoot" prefHeight="1000.0" prefWidth="1600.0" styleClass="mainFxmlClass">
          <stylesheets>
              <URL value="@userDetails.css" />
          </stylesheets>
         <children>
            <ImageView fitHeight="1010.0" fitWidth="1604.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@userDetailsScreen.PNG" />
               </image>
            </ImageView>
            <Label fx:id="userScreenLabel" layoutX="766.0" layoutY="307.0" prefHeight="36.0" prefWidth="278.0" />
            <TextField fx:id="textFieldUsername" layoutX="713.0" layoutY="371.0" prefHeight="54.0" prefWidth="251.0" promptText="Enter Username" />
            <TextField fx:id="textFieldAge" layoutX="711.0" layoutY="490.0" prefHeight="51.0" prefWidth="259.0" promptText="Enter Age" />
            <Label layoutX="475.0" layoutY="364.0" prefHeight="59.0" prefWidth="201.0" text="Username">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Label>
            <Label layoutX="457.0" layoutY="491.0" prefHeight="40.0" prefWidth="226.0" text="Age">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Label>
            <ImageView fx:id="proceedButton" fitHeight="52.0" fitWidth="164.0" layoutX="764.0" layoutY="615.0" onMouseClicked="#user_proceed" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@proceedButton.png" />
               </image>
            </ImageView>
            <ImageView fx:id="backButton" fitHeight="68.0" fitWidth="139.0" layoutX="26.0" layoutY="22.0" onMouseClicked="#user_back" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@backButton.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</StackPane>
