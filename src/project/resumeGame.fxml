<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="container" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.resumeGameController">
   <children>
      <AnchorPane id="AnchorPane" fx:id="anchorRoot" prefHeight="750.0" prefWidth="1200.0" styleClass="mainFxmlClass">
          <stylesheets>
              <URL value="@resumeGame.css" />
          </stylesheets>
         <children>
            <ImageView fitHeight="766.0" fitWidth="1199.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@mainScreen.jpg" />
               </image>
            </ImageView>
            <ImageView fx:id="backButton" fitHeight="48.0" fitWidth="116.0" layoutX="32.0" layoutY="31.0" onMouseClicked="#user_back" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@backButton.png" />
               </image>
            </ImageView>
            <ComboBox fx:id="chooseUser" layoutX="522.0" layoutY="383.0" prefHeight="37.0" prefWidth="180.0" promptText="Choose User" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
